{
  "hosting": {
    "source": ".",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "frameworksBackend": {
      "region": "us-east1"
    },
    "headers": [
      {
        "source": "**/*.{woff,woff2,ttf,otf,eot}",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public,max-age=31536000,immutable"
          }
        ]
      },
      {
        "source": "**/*.{js,css,webp,png,jpg,jpeg,svg,gif,avif,ico,webmanifest}",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public,max-age=604800,immutable"
          }
        ]
      }
    ]
  }
}
